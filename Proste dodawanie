#include <iostream>
using namespace std;
 
int main()
{
int t, n, a, score;
int tb[100];
 
    cin >> t;                   
    for(int i=0;i<t;++i)
    {
           score=0;
           cin>>n;
           for(int j=0;j<n;++j)
           {
                cin>>a;
                score += a;
           }
           tb[i]=score;
    }
    for(int k=0;k<t;++k)
           cout<<tb[k]<<"\n";
 
} 
